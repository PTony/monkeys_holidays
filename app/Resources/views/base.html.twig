            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        

    </div>
    <!-- Collection of nav links, forms, and other content for toggling -->
    <div id="navbarCollapse" class="collapse navbar-collapse">
        <ul class="nav navbar-nav">
            <li class=""><a href="#">Créer ton groupe d'amis  <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
             
            <li><a href="#"> Mes groupes <img src="{{ asset('/images/liste.png') }}"  alt="logo" class="" /></a></li>
            <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle" href="#">Créer <b class="caret"></b></a>
                <ul role="menu" class="dropdown-menu">
                    <li><a href="#"> Client </a></li>
                    <li><a href="#"> Organisme</a></li>
                    <li><a href="#"> École</a></li>
                    <li><a href="#">Document</a></li>
                    <li><a href="#">Produit</a></li>                
                </ul>
            </li>
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">Éditer <b class="caret"></b></a>
                    <ul role="menu" class="dropdown-menu">
                        <li><a href="#"> Client </a></li>
                        <li><a href="#"> Organisme</a></li>
                        <li><a href="#"> École</a></li>
                        <li><a href="#">Document</a></li>
                        <li><a href="#">Produit</a></li>                
                    </ul>
                </li>
        </ul>
        
        <ul class="nav navbar-nav navbar-right">
            <li><a href="#">Se déconnecter <img src="{{ asset('/images/logout.png') }}"  alt="logo" class="" /> </a></li>
        </ul>
    </div>
</nav>
        <a href="./">
          {# <img style="position:fixed;top:7%;left:0%;right:0%;height:100%;width:100%;z-index:-1:" src="{{ asset('/images/sea.jpg') }}"/>  #}
         </a>

        {% block body %}{% endblock %}
        
        {% block javascripts%}
        <script type="text/javascript" src="{{ asset('js/jquery.js') }}" ></script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}" ></script>
        {% endblock %}
        
    </body>
</html>
