{% extends 'base.html.twig' %}

{% block body %}
    <h1>Appartenir list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for appartenir in appartenirs %}
            <tr>
                <td><a href="{{ path('appartenir_show', { 'id': appartenir.id }) }}">{{ appartenir.id }}</a></td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('appartenir_show', { 'id': appartenir.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('appartenir_edit', { 'id': appartenir.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('appartenir_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
